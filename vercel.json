{
    "version": 2,
    "framework": "vite",
    "functions": {
        "src/api/index.js": {
            "runtime": "vercel-node@latest",
            "memory": 1024,
            "maxDuration": 60
        }
    },
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/src/api/index.js"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}